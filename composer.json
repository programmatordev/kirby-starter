{
  "name": "programmatordev/kirby-starter",
  "description": "A very opinionated Kirby CMS development stack with DDEV, Vite, Blade, Alpine.js and Tailwind CSS",
  "type": "project",
  "keywords": ["kirby", "kirby-cms", "tailwindcss", "ddev", "vite", "alpine-js", "blade"],
  "authors": [
    {
      "name": "Programmator",
      "email": "hotline@programmator.dev",
      "homepage": "https://programmator.dev"
    }
  ],
  "require": {
    "php": ">=8.3.0",
    "getkirby/cms": "^5.2",
    "arnoson/kirby-vite": "^5.6",
    "beebmx/kirby-env": "^5.1",
    "tobimori/kirby-seo": "^2.0.0-alpha.9",
    "moritzebeling/kirby-favicon": "^1.0",
    "leitsch/kirby-blade": "^3.1"
  },
  "require-dev": {
    "symfony/var-dumper": "^7.4"
  },
  "autoload": {
    "psr-4": {
      "App\\": "site/classes/"
    }
  },
  "config": {
    "allow-plugins": {
      "getkirby/composer-installer": true
    },
    "optimize-autoloader": true
  },
  "scripts": {
    "start": [
      "Composer\\Config::disableProcessTimeout",
      "@php -S localhost:8000 kirby/router.php"
    ]
  }
}
